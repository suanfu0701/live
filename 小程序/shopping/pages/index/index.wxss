/* 楼层 */
.floor_title{
  padding: 10rpx 0;
}
.title_img{
  width: 100%;
}
.floor_list{
  overflow: hidden;
}
.floor_nav{
  float: left;
  width: 33.33%;
}
 /* 后四个超链接 */
.floor_nav:nth-last-child(-n+4){
  /* 原图的宽高 232*386 */
  /* 第一张图片高度 height：33.33vw*386/232 */
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
/* 2 3 两个超链接 */
.floor_nav:nth-child(2),
.floor_nav:nth-child(3){
  border-bottom: 10rpx solid #fff; 
}
/* 使其图片继承父元素大小 */
.floor_nav_img1{
  width: 100%;
  height: 100%;
}
